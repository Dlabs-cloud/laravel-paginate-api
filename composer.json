{
  "name": "dlabs.cloud/paginate-api",
  "description": "If you will like to use offset with your query param in a request and get a simple pagination response, this will give you all out of the box",
  "type": "library",
  "authors": [
    {
      "name": "Oluwatobi Adenekan",
      "email": "tobi@dlabs.cloud"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "5.8.*",
    "illuminate/http": "5.8.*",
    "illuminate/database": "5.8.*",
    "illuminate/pagination": "5.8.*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dlabs\\PaginateApi\\PaginateApiServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Dlabs\\PaginateApi\\": "src/"
    }
  }
}
